function UI_UgcComLevelDetails:SetInfo(info)
    if info then
        self.TitlePlayerNum.text=info.playCount
        self.TitleTokenNum.text=info.collect
        self.TitleSupportNum.text=info.like
        self.TitlePassNum.text=string.format("%.2f",info.passRate)..'%'
        if info.onlineTime then
            self.TitleDayUpNum.text=CS.GUIHelper.FormatUnixToDate(info.onlineTime)
        end

  --      self.TitleDayDownNum.text=info.offlineTime
    end
end

function UI_UgcComLevelDetails:Dispose()
    self.TitlePlayerNum=nil
    ---工厂收益
    self.TitleTokenNum=nil
    ---点赞人数
    self.TitleSupportNum=nil
    ---通关率
    self.TitlePassNum=nil
    ---上架时间
    self.TitleDayUpNum=nil
end